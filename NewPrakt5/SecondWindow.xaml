<Window x:Class="NewPrakt5.SecondWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewPrakt5"
        mc:Ignorable="d"
        Title="SecondWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="pokypka">
                <Grid>
                    <DataGrid x:Name="PokypkaDG" d:ItemsSource="{d:SampleData ItemCount=5}" />
                    <Button x:Name="chek"  Content="Вывести чек" HorizontalAlignment="Left" Margin="84,232,0,0" VerticalAlignment="Top" Height="47" Width="114" Click="chek_Click"/>
                    <Button Content="Вернуться к авторизации" HorizontalAlignment="Left" Margin="600,345,0,0" VerticalAlignment="Top" Height="35" Width="160" Click="Button_Click"/>

                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
